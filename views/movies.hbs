
<div id="title">
    <h1>Ironhack Cinema</h1>
    <h3>Click on the movie to check the showtimes!</h3>
</div>

<div id="catalogo">
    {{#each response}}  
        <article>
            <img src="{{this.image}}" alt="{{this.title}}" />
            <br/><h3>{{this.title}}</h3>
            <br/><a href="/movies/{{this._id}}">See More</a>        
        </article>
    {{/each}}
       

</div>